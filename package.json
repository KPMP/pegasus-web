  "scripts": {
    "start": "npm-run-all --max-old-space-size=8192 -p watch-css start-js",
    "build": "npm-run-all --max-old-space-size=10240 build-css build-js",
    "test": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
